{
	"string": [
		{
			"name": "unicode",
			"match": "\""
		}, {
			"name": "ascii",
			"match": "'"
		}
	],
	"comment": [
		["//", "\n"],
		["/*", "*/"]
	],
	"token": {
		"add"      : "+",
		"subtract" : "-",
		"divide"   : "/",
		"multiply" : "*",
		"modulo"   : "%",

		"and"        : "&",
		"or"         : "|",
		"not"        : "!",
		"greatEqual" : "<=",
		"lessEqual"  : ">=",
		"aproxEqual" : "~=",
		"equalTo"    : "==",

		"assign": "=",
		"semicolon": ";",

		"pipe": "=>"
	},
	"bracket": {
		"curley" : ["{", "}"],
		"round"  : ["(", ")"],
		"square" : ["[", "]"]
	},
	"keyword": [
		"return",
		"await",
		"class",
		"public",
		"private",
		"for",
		"while"
	],


	"pattern": [
		{
			"name": "import",
			"match": ["keyword.import", "string.unicode", "keyword.as", "namespace", "token.semicolon"],
			"sub": [null, null, null, null, null]
		},{
			"name": "import",
			"match": ["keyword.import", "string.unicode", "keyword.as", "token.multiply", "token.semicolon"],
			"sub": [null, null, null, null, null]
		},{
			"name": "import",
			"match": ["keyword.import", "string.ascii", "keyword.as", "namespace", "token.semicolon"],
			"sub": [null, null, null, null, null]
		},{
			"name": "import",
			"match": ["keyword.import", "string.ascii", "keyword.as", "token.multiply", "token.semicolon"],
			"sub": [ null, null, null, null, null ]
		},{
			"name": "export",
			"match": [ "keyword.export", "namespace" ],
			"sub": null
		},

		{
			"name": "expression",
			"match" : ["*"],
			"sub"   : null
		},{
			"name": "code",
			"match" : ["*"],
			"sub"   : [
				["definition", "definition.assign", "return"]
			]
		},



		{
			"name": "definition",
			"match" : ["namespace", "namespace"],
			"sub"   : [ null,        null]
		},{
			"name": "definition.assign",
			"match" : ["namespace", "namespace", "token.equal", "*",         "token.semicolon"],
			"sub"   : [
				null,
				null,
				null,
				["expression"],
				null
			]
		},{
			"name": "assign",
			"match" : ["namespace", "token.equal", "*",         "token.semicolon"],
			"sub"   : [
				null,
				null,
				["expression"],
				null
			]
		},



		{
			"name": "function",
			"match" : ["namespace", "namespace", "bracket.round", "bracket.curley"],
			"sub"   : [
				null,
				null,
				["definition", "definition.assign"],
				["code"]
			]
		},



		{
			"name": "modifier.public",
			"match": ["keyword.public", "token.semicolon"],
			"sub": null
		},{
			"name": "modifier.private",
			"match": ["keyword.private", "token.semicolon"],
			"sub": null
		},{
			"name": "class",
			"match" : ["keyword.class", "namespace", "bracket.curley"],
			"sub"   : [
				null, null,
				["definition", "definition.assign", "function"]
			]
		},



		{
			"name": "return",
			"match": [
				["keyword.return", "*", "token.semicolon"]
			]
		}
	],

	"root": [
		"class",
		"function",
		"import",
		"export"
	]
}
